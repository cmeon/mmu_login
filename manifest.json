{
  "name": "Auto MMU Wifi Login",
  "description": "Auto login when on the MMU wifi",
  "version": "0.0.3",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "promo/icon-16.png",
	     "48": "promo/icon-48.png",
             "128": "promo/icon-128.png" },
  "permissions": [
    "https://wifi.mmu.edu.my/login.html",
    "storage"
  ]
}
